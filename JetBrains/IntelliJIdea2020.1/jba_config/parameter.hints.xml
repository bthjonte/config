<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript" />
      <blacklist language="Java" />
      <blacklist language="Groovy" />
      <blacklist language="Kotlin">
        <added pattern="javax.ws.rs.Path(value)" />
        <added pattern="org.glassfish.jersey.client.ClientConfig.property(name,value)" />
        <added pattern="org.junit.Assert.assertEquals(expected,actual)" />
        <added pattern="javax.ws.rs.client.Client.target(uri)" />
        <added pattern="UserEndpointTest.responseTest(testText,Location)" />
      </blacklist>
    </blacklists>
    <option id="SHOW_PARAMETER_HINT" value="false" />
    <option id="SHOW_LAMBDA_IMPLICIT_PARAMETER_RECEIVER" value="false" />
    <option id="SHOW_LAMBDA_RETURN_EXPRESSION" value="false" />
  </component>
</application>