<toolSet name="External Tools">
  <tool name="fuser" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="true" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="fuser" />
      <option name="PARAMETERS" value="-k 50000/tcp" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="prettier" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="true" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="prettier " />
      <option name="PARAMETERS" value=" --config $ProjectFileDir$/.prettierrc.js" />
      <option name="WORKING_DIRECTORY" value="$Projectpath$" />
    </exec>
  </tool>
</toolSet>